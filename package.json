{
  "name": "chrono-flow",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint --ext .ts,.tsx src --max-warnings=0 --format json -o eslint-report.json",
    "lint:fix": "eslint --ext .ts,.tsx src --fix",
    "lint:pretty": "eslint --ext .ts,.tsx src --format stylish",
    "typecheck": "tsc --noEmit",
    "preview": "vite preview",
    "test": "vitest",
    "test:watch": "vitest",
    "test:ci": "vitest run --coverage --reporter=dot",
    "sast": "semgrep --config p/typescript --config p/react --error",
    "sca": "npm audit --json > audit.json || true; node scripts/check-vulns.js"
